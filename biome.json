{
	"$schema": "https://biomejs.dev/schemas/1.7.0/schema.json",
	"organizeImports": {
		"enabled": true
	},
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true,
			"suspicious": {
				"noArrayIndexKey": "off",
				"noAssignInExpressions": "off"
			},
			"complexity": {
				"noForEach": "off"
			},
			"style": {
				"noParameterAssign": "off"
			}
		}
	}
}
